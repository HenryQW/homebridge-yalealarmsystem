	"author": "Jonathan Dann",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jonathandann/homebridge-yalesyncalarm/issues"
	},
	"homepage": "https://github.com/jonathandann/homebridge-yalesyncalarm#readme",
	"engines": {
		"homebridge": ">=0.4.5"
	},
	"dependencies": {
		"type-safe-json-decoder": "^0.2.0",
		"yalesyncalarm": "^2.0.1"
	},
	"devDependencies": {
		"hap-nodejs": "^0.5.6",
		"prettier": "^1.19.1",
		"typescript": "^3.7.5"
